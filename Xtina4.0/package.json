{
  "name": "xtina-4.0",
  "productName": "Xtina AI Crew Commander",
  "version": "4.0.0",
  "description": "Enterprise AI Crew Commander Suite - Multi-Agent Orchestration Platform with 80+ API Integrations",
  "main": "backend/electron.js",
  "author": "Xtina Development Team",
  "license": "MIT",
  "homepage": "https://github.com/lindenchris206/Xtina",
  "repository": {
    "type": "git",
    "url": "https://github.com/lindenchris206/Xtina.git"
  },
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "nodemon backend/server.js",
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "dev:electron": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build:frontend": "tsc && vite build",
    "build:backend": "tsc backend/**/*.ts --outDir backend/dist",
    "build": "npm run build:frontend && npm run build:backend",
    "preview": "vite preview",
    "start": "node backend/server.js",
    "electron:build": "npm run build && electron-builder",
    "electron:build:win": "npm run build && electron-builder --win",
    "electron:build:mac": "npm run build && electron-builder --mac",
    "electron:build:linux": "npm run build && electron-builder --linux",
    "electron:build:all": "npm run build && electron-builder -mwl",
    "android:build": "cap sync android && cd android && ./gradlew assembleRelease",
    "android:dev": "cap sync android && cap run android",
    "ios:build": "cap sync ios && cd ios/App && xcodebuild",
    "ios:dev": "cap sync ios && cap run ios",
    "package:all": "npm run electron:build:all && npm run android:build",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\""
  },
  "dependencies": {
    "@google/genai": "^1.24.0",
    "@capacitor/android": "^5.5.1",
    "@capacitor/core": "^5.5.1",
    "@capacitor/ios": "^5.5.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.0",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "react-router-dom": "^6.21.0",
    "zustand": "^4.4.7",
    "openai": "^4.20.1",
    "anthropic": "^0.10.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.5.1",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "electron": "^28.1.3",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.2.1",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.xtina.aicrew",
    "productName": "Xtina 4.0",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "frontend/dist/**/*",
      "backend/**/*",
      "!backend/**/*.ts",
      "backend/dist/**/*",
      "shared/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "frontend/dist",
        "to": "app",
        "filter": ["**/*"]
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "ia32"]
        },
        {
          "target": "portable",
          "arch": ["x64"]
        }
      ],
      "icon": "assets/icon.ico",
      "artifactName": "Xtina-4.0-Setup-${arch}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Xtina 4.0",
      "installerIcon": "assets/icon.ico",
      "uninstallerIcon": "assets/icon.ico",
      "installerHeaderIcon": "assets/icon.ico"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": ["x64", "arm64"]
        },
        {
          "target": "zip",
          "arch": ["x64", "arm64"]
        }
      ],
      "category": "public.app-category.developer-tools",
      "icon": "assets/icon.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "artifactName": "Xtina-4.0-${arch}.${ext}"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "title": "Xtina 4.0 Installer",
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": [
        {
          "target": "deb",
          "arch": ["x64", "arm64"]
        },
        {
          "target": "AppImage",
          "arch": ["x64", "arm64"]
        },
        {
          "target": "rpm",
          "arch": ["x64"]
        },
        {
          "target": "snap",
          "arch": ["x64"]
        }
      ],
      "category": "Development",
      "icon": "assets/icon.png",
      "synopsis": "AI Crew Commander Suite",
      "description": "Enterprise AI Crew Commander with 80+ API integrations, multi-agent orchestration, and secure credential management",
      "artifactName": "Xtina-4.0-${arch}.${ext}"
    },
    "deb": {
      "depends": [
        "gconf2",
        "gconf-service",
        "libnotify4",
        "libappindicator1",
        "libxtst6",
        "libnss3"
      ],
      "priority": "optional",
      "afterInstall": "build/linux-installer.sh"
    },
    "snap": {
      "confinement": "classic",
      "grade": "stable"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "lindenchris206",
        "repo": "Xtina"
      }
    ]
  },
  "capacitor": {
    "appId": "com.xtina.aicrew",
    "appName": "Xtina AI Crew",
    "webDir": "frontend/dist",
    "server": {
      "androidScheme": "https"
    }
  }
}
